	Doi = {10.1093/mnras/173.3.729},
	Journal = {\mnras},
	Keywords = {Binary Stars, Many Body Problem, Stellar Evolution, Equations Of Motion, Kinetic Energy, Numerical Integration, Stellar Motions},
	Month = dec,
	Pages = {729-787},
	Title = {{Binary evolution in stellar dynamics}},
	Volume = 173,
